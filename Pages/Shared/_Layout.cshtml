                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/States/Index">State</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Voltages/Index">Voltage</a>
                        </li>
                    </ul>
                </div>
            
    </div>
    <div class="container">
        <main role="main" class="padding_container">
            @RenderBody()
        </main>
    </div>
    </div>
 

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - EMS - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
